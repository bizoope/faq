body{background-color:#fff;color:#010101}a:link{color:var(--link-color);text-decoration:none}a:visited{color:var(--link-color);text-decoration:none}a:hover{text-decoration:underline}#tags ul{max-width:var(--wide-large);margin:0 auto;display:flex;justify-content:center;flex-wrap:wrap;list-style:none}#tags ul li a{display:block;padding:0 10px;background-color:#dddfdf;font-size:14px}#contentsList{max-width:var(--wide-large);margin:0 auto;padding:20px 0 300px;text-align:left}#contentBody{max-width:var(--wide-large);margin:0 auto;padding:20px 0;text-align:left}#categories ul{max-width:var(--wide-large);margin:0 auto;display:flex;justify-content:center;flex-wrap:nowrap;list-style:none}#categories ul li a{display:block;margin:10px;font-size:20px}#searchBox{max-width:var(--wide-large);margin:50px auto 20px}#searchBox #searchBoxIcon{height:25px;width:25px;top:5px;right:15px}#searchBox #searchBoxInput{width:var(--wide-large);height:120%}#searchResults{top:200px}