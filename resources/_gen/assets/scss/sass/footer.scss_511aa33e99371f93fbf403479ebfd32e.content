.footer{width:1000px;font-size:15px;background:#fff;border-top:3px solid #a4a29e;position:absolute;bottom:0;left:calc(50% - 1000px/2);height:100px;text-align:center;display:flex;padding:0 0 30px}.footer_logo{padding:20px 0 0;text-align:left}.footer_list{text-align:center;padding:0 0 0 170px}.footer_title{text-align:center;font-weight:var(--wide-large)}.footer_navi{list-style:none;display:flex;flex-wrap:wrap;justify-content:center}.footer_navi li{padding:0 20px}